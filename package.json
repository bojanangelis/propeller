  "scripts": {
    "start:dev": "env-cmd -f .env concurrently --kill-others \"yarn:db:up\" \"yarn:dev:api\" \"yarn:dev:web\"",
    "dev:api": "nx serve api",
    "dev:web": "nx serve web",
    "DATABASE": "-----------------------------------------------------------------------------------------------",
    "db:up": "yarn db:docker && yarn db:dev-migrate && yarn db:create-client -- --watch",
    "start:db:watch": "yarn run db:dockers:dev  && yarn run db:migrate:dev && yarn run db:seed:dev && prisma generate --watch",
    "db:docker": "docker-compose -f tools/docker/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:create-client": "prisma generate",
    "db:dev-migrate": "env-cmd --no-override -f .env npx prisma migrate dev",
    "db:studio": "env-cmd --no-override -f .env npx prisma studio",
    "db:seed:dev": "npx env-cmd -f .env ts-node tools/database/seed.dev.ts"
  },
  "private": true,
